# Practical 10:
# Define class Point with attributes x and y. Create methods and print objects.
# Define method distance to calculate distance between two point objects.
# Accepts coordinates from user for demonstration.

import math

class Point:
    def __init__(self, x=0.0, y=0.0):
        self.x = float(x)
        self.y = float(y)

    def __repr__(self):
        return f"Point({self.x}, {self.y})"

    def distance(self, other):
        return math.hypot(self.x - other.x, self.y - other.y)

def main():
    x1 = float(input("Enter x for Point A: "))
    y1 = float(input("Enter y for Point A: "))
    x2 = float(input("Enter x for Point B: "))
    y2 = float(input("Enter y for Point B: "))
    p1 = Point(x1, y1)
    p2 = Point(x2, y2)
    print("Point A:", p1)
    print("Point B:", p2)
    print("Distance between A and B:", p1.distance(p2))

if __name__ == "__main__":
    main()
